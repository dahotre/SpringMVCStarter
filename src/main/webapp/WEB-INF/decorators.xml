<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/decorator/">
  <!-- Excludes will never be decorated by Sitemesh. -->
  <excludes>
    <pattern>*/exclude/*</pattern>
    <pattern>*.txt</pattern>
    <pattern>*.css</pattern>
    <pattern>*.js</pattern>
  </excludes>

  <decorator name="basic-theme" page="basic-theme.jsp">
    <pattern>/*</pattern>
  </decorator>
</decorators>